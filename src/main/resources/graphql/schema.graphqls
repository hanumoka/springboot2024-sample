type Query {
    getBoards: [BoardEntity!]!
    getBoardsConnection(first: Int, after: String): BoardConnection!
}

type BoardEntity {
    id: ID!
    title: String!
    content: String!
}

type BoardConnection {
    edge: [BoardEdge!]!
    pageInfo: PageInfo!
}


type BoardEdge {
#    node: BoardEntity!
    cursor: String!
}

type PageInfo {
    hasNextPage: Boolean!
    endCursor: String
}


